{
  "indentWidth": 2,
  "typescript": {},
  "json": {},
  "markdown": {},
  "toml": {},
  "dockerfile": {},
  "newLineKind": "auto",
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,dockerfile,sql}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/.yarn",
    "**/yarn.lock",
    "**/dist"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.69.5.wasm",
    "https://plugins.dprint.dev/json-0.15.3.wasm",
    "https://plugins.dprint.dev/markdown-0.13.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.3.wasm",
    "https://plugins.dprint.dev/prettier-0.10.0.json@ca9a97de84cbb2cd60534eb72c0455f3ca8704743917569ace70499136cf5c9c",
    "https://plugins.dprint.dev/sql-0.1.2.wasm"
  ],
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "proseWrap": "always"
  }
}
